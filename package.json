{
  "name": "running-tools",
  "version": "1.0.41",
  "scripts": {
    "ng": "ng",
    "start": "npm run set-version && ng serve",
    "build": "npm run set-version && ng build",
    "build:debug": "ng build --verbose > ng-build.log 2>&1",
    "build:ssr": "npm run set-version && ng build --configuration production",
    "serve:ssr": "node dist/running-tools/server/server.mjs",
    "watch": "npm run set-version && ng build --watch --configuration development",
    "test": "ng test",
    "lint": "ng lint",
    "set-version": "node scripts/set-version.js",
    "version:patch": "npm version patch && npm run set-version",
    "version:minor": "npm version minor && npm run set-version",
    "version:major": "npm version major && npm run set-version",
    "build:prod": "npm version patch --no-git-tag-version && npm run set-version -- --commit && ng build --configuration production",
    "build:prod-no-commit": "npm version patch --no-git-tag-version && npm run set-version && ng build --configuration production",
    "build:prod-debug": "ng build --configuration production"
  },
  "private": true,
  "dependencies": {
    "@angular/cdk": "^20.0.2",
    "@angular/common": "^20.0.0",
    "@angular/compiler": "^20.0.0",
    "@angular/core": "^20.0.0",
    "@angular/fire": "^20.0.1",
    "@angular/forms": "^20.0.0",
    "@angular/material": "^20.0.2",
    "@angular/platform-browser": "^20.0.0",
    "@angular/platform-server": "^20.0.0",
    "@angular/router": "^20.0.0",
    "@angular/service-worker": "^20.0.0",
    "@angular/ssr": "^20.0.1",
    "@ngneat/tailwind": "^7.0.3",
    "@ngx-translate/core": "^16.0.4",
    "@ngx-translate/http-loader": "^16.0.1",
    "@tailwindcss/cli": "^4.1.8",
    "@tailwindcss/postcss": "^4.1.8",
    "express": "^5.1.0",
    "firebase": "^11.10.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^20.0.1",
    "@angular/build": "^20.0.1",
    "@angular/cli": "^20.0.1",
    "@angular/compiler-cli": "^20.0.0",
    "@types/express": "^5.0.1",
    "@types/jasmine": "~5.1.0",
    "@types/node": "^20.17.19",
    "angular-eslint": "19.8.0",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.28.0",
    "jasmine-core": "~5.7.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "postcss": "^8.5.4",
    "prettier": "^3.5.3",
    "tailwindcss": "^4.1.8",
    "typescript": "~5.8.2",
    "typescript-eslint": "8.33.1"
  }
}
